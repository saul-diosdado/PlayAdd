{
    "name": "PlayAdd for Spotify",
    "version": "0.4",
    "description": "Add YouTube songs to Spotify playlists.",
    "permissions": ["tabs", "activeTab", "storage"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "login.html"
    },
    "manifest_version": 2
}